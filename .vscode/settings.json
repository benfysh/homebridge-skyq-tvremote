{
  "files.eol": "\n",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.rulers": [ 140 ],
  "eslint.enable": true,
  "ponicode.testSettings.testLocation.locationType": "Same folder as source file",
  "ponicode.testSettings.testLocation.path": "{rootDir}/{filePath}/{fileName}.test.{ext}"
}